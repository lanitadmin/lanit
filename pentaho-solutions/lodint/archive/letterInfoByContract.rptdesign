" aCard.docType, "+
" aCard.docDate, "+
" ( select "+
	" ce.name "+
	" from "+
	" AdditionalFieldValue afv1, CardElement ce "+ 
	" where "+
	" afv1.field.name = 'status' "+
	" and afv1.itemId = aColl.id and ce.id= afv1.stringValue "+
	" and afv1.field.model='org.lanit.samara.darchive.ArchiveCollection' ), "+
"( select "+
	" afv2.dateValue "+
	" from "+
	" AdditionalFieldValue afv2 "+ 
	" where "+
