paramStateNameProj = clazz.getEntityManagerLod().createQuery(
" select name from CardElement where id = '"+params["stateList"]+"'"
).getSingleResult();

contractDocsList = clazz.getEntityManagerLod().createQuery(

" select "+ 
" aCard.docType, "+ 
" aCard.docDate,  "+
" (select  "+
"	 afv3.booleanValue "+ 
"	 from  "+
"	 AdditionalFieldValue afv3 "+ 
"	 where  "+
"	 afv3.field.name = 'signed' "+ 
"	 and afv3.itemId = aCard.id  "+
"	 and afv3.field.model='org.lanit.samara.darchive.ArchiveCard' ), "+ 
"(select ce.name   "+
"	 from    "+
"	 ArchiveHistory ah, CardElement ce "+ 
"	 where    "+
"	 ah.card.id = aCard.id and "+ 
"	 ce.id = ah.status    "+
"	 and ah.date = (select max(ah1.date) from ArchiveHistory ah1 "+ 
"	 where   "+
"	 ah1.card.id = ah.card.id)), "+ 
" aCard.docName, "+
//"( select name from CardElement where "+ 
//"			 card.cardCode = 'SDocType' and id = aCard.docType ), "+
" aCard.docNumber "+
" from "+ 
" ArchiveCard aCard "+ 
" where "+
" aCard.collection.id="+localParam

).getResultList();

totalrows = contractDocsList.size();

currentrow = 0;]]></method>
            <method name="fetch"><![CDATA[

if (currentrow >= totalrows) {
	return(false);
}
var rowValue = contractDocsList.get(currentrow);
if ("letter".equals(rowValue[0])){
cardState = rowValue[3];
if (cardState==null) isEequealState = 0; 
else isEequealState = cardState.equals(paramStateNameProj);

if (params["useStateSearch"]==0) isEequealState=1;

if (isEequealState){
docName = "";
if (rowValue[4]!=null &amp;&amp; !"".equals(rowValue[4])) docName = rowValue[4];
if (rowValue[5]!=null &amp;&amp; !"".equals(rowValue[5])) docName += " №"+rowValue[5];
row["docName"] = docName;
row["docDate"] =rowValue[1];
if (rowValue[2]==0 ) row["isSigned"] ="нет";
else row["isSigned"] ="да";

if (cardState==null) row["status"] = "[не задано]"; 
else row["status"] = cardState;


row["countRows"]=contractDocsList.size();
}
}

currentrow = currentrow + 1;

return(true);
]]></method>
