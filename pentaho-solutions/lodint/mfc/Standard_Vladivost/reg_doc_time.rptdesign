                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">3</property>
                    <property name="name">gettingTime</property>
                    <property name="dataType">integer</property>
                </structure>
            </list-property>
            <list-property name="columnHints">
                <structure>
                    <property name="columnName">service</property>
                </structure>
                <structure>
                    <property name="columnName">operator</property>
                </structure>
                <structure>
                    <property name="columnName">gettingTime</property>
                </structure>
            </list-property>
            <structure name="cachedMetaData">
                <list-property name="resultSet">
                    <structure>
                        <property name="position">1</property>
                        <property name="name">service</property>
                        <property name="dataType">string</property>
                    </structure>
                    <structure>
                        <property name="position">2</property>
                        <property name="name">operator</property>
                        <property name="dataType">string</property>
                    </structure>
                    <structure>
                        <property name="position">3</property>
                        <property name="name">gettingTime</property>
                        <property name="dataType">integer</property>
                    </structure>
                </list-property>
            </structure>
            <property name="dataSource">Data Source</property>
            <method name="open"><![CDATA[clazz = new Packages.org.cp.birt.plugins.Shark();
clazzConvert = new Packages.org.cp.birt.plugins.Converter();

startDate = clazzConvert.getOutDate(params["startDate"]);
endDate = clazzConvert.getOutDate(params["endDate"]); 

result = clazz.getEntityManagerLod().createQuery(

" select  "+
" ce.name, "+
" ps1.begins, "+
" ps2.ends, "+
" u.lastName || ' ' || u.firstName || ' ' || u.middleName "+
" from GossrvcDocPackage dp join dp.registrator  u join u.roles role, PackStatus ps1, PackStatus ps2, CardElement ce "+
" where  (u.test = false or u.test is null) "+ 
" and role.code like 'Registrator%' "+ 
" and ps1.id = (select min(ps.id)from PackStatus ps inner join ps.status st join ps.gossrvcDocPackage dp1 "+
"               where st.code = 'regKomplDoc' "+ 
"             and ps.begins between :startDate and :endDate "+ 
"               and dp1.id = dp.id ) "+ 
" and ps2.id = (select min(ps.id)from PackStatus ps inner join ps.status st join ps.gossrvcDocPackage dp1 "+
"               where st.code = 'forRaspred' "+ 
"               and dp1.id = dp.id ) "+ 
" and ce.id = dp.serviceId "+
"   AND dp.testPackage = false " 
).setParameter("startDate", startDate)
.setParameter("endDate", endDate) 
.getResultList();



totalrows = result.size();



currentrow = 0;]]></method>
            <method name="fetch"><![CDATA[if (currentrow >= totalrows) {
	return(false);
}

var rowValue = result.get(currentrow);

row["service"] = rowValue[0];
row["operator"] = rowValue[3];
row["gettingTime"] = (rowValue[2].getTime()-rowValue[1].getTime())/1000/60;

currentrow = currentrow + 1;

return(true);]]></method>
            <method name="close"><![CDATA[result=null;]]></method>
        </script-data-set>
