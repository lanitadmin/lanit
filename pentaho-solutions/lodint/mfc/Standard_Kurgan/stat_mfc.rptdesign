            <method name="fetch"><![CDATA[if (currentrow >= totalrows) {
	return(false);
}

countYears = countYears+1;
years = (currentYear-10 + countYears);
row["YEARS"] = years;

currentrow = currentrow + 1;
return(true);]]></method>
