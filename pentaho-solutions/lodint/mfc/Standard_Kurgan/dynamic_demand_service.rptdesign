            <property name="dataSource">Data Source</property>
            <method name="open"><![CDATA[clazzShark = new Packages.org.cp.birt.plugins.Shark();
clazzConvert = new Packages.org.cp.birt.plugins.Converter();

startDate = clazzConvert.getOutDate(params["startDate"]);
endDate = clazzConvert.getOutDate(params["endDate"]);

result = clazzShark.getEntityManagerShark().createQuery(
" select ogv.name, level.name, service.name" + 
" from PackStatus ps, OrgUnit ogv, CardElement level, CardElement service, CardElementFieldValue cefv" +
" where ps.status.code = 'forRaspred' and ps.owner = ogv.owner" +
" and ps.ends is not null" +
" and level.id = TO_CHAR(cefv.fieldStringValue)" +
" and ps.gossrvcDocPackage.serviceId = service.id" +
" and cefv.cardElement.id = ps.gossrvcDocPackage.serviceId" +
" and cefv.cardField.fieldName = 'serviceGovLevel'" +
" and ps.begins between :startDate and :endDate")
.setParameter("startDate", startDate)
.setParameter("endDate", endDate)
.getResultList();

totalrow = result.size();
currentrow = 0;]]></method>
            <method name="fetch"><![CDATA[if (currentrow > totalrow) {
	return (false);
}

var rowValue = result.get(currentrow);

row["ogv"] = rowValue[0];
row["level"] = rowValue[1];
row["service"] = rowValue[2];

currentrow = currentrow + 1;

return(true)]]></method>
