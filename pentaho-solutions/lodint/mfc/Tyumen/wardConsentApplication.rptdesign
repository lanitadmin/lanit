            <method name="open"><![CDATA[clazz = new Packages.org.cp.birt.plugins.Shark();
clazzShark = new Packages.org.cp.birt.plugins.HibernateUtil("entityManagerNoFilter");
result2 = clazz.getActivityVariableValue
    (
        "Zayavitel,Komplekt_doc,Service,Zayavlenie,selectOwner,dateServEnd",
        params["processId"],
        params["activityId"]
    );

Zayavitel = result2[0];
Komplekt_doc = result2[1];
Zayavlenie = result2[3];

result = clazz.getEntityManagerLod().createQuery(
" SELECT "+

"   ( " +
"       SELECT P.lastName || ' ' || P.firstName || ' ' || P.middleName " +
"       FROM Person as P " +
"       WHERE ogi.objectClass = 'org.cp.model.common.Person' " +
"       AND P.id = ogi.objectId " +
"   ) as pName, " +
"   ( " +
"       SELECT P.factAddress FROM Person as P " +
"       WHERE ogi.objectClass = 'org.cp.model.common.Person' " +
"       AND P.id = ogi.objectId " +
"   ) as pAddress, " +
"   ( " +
"       SELECT P.passSeria FROM Person as P "+
"       WHERE ogi.objectClass='org.cp.model.common.Person' AND P.id = ogi.objectId " +
"   ) as pDocSeries, " +
"   ( " +
"       SELECT P.passNo FROM Person as P " +
"       WHERE ogi.objectClass='org.cp.model.common.Person' AND P.id = ogi.objectId " +
"   ) as pDocNumber, " +
"   ( " +
"       SELECT P.passDate FROM Person as P " +
"       WHERE ogi.objectClass='org.cp.model.common.Person' AND P.id = ogi.objectId " +
"   ) as pDocIssueDate, " +
"   ( " +
"       SELECT P.passFrom FROM Person as P " +
"       WHERE ogi.objectClass='org.cp.model.common.Person' AND P.id = ogi.objectId " +
"   ) as pDocIssueOrg, " +
"   ( " +
"       SELECT A.person.lastName || ' ' || A.person.firstName || ' ' || A.person.middleName " +
"       FROM Agent as A " +
"       WHERE ogi.objectClass = 'org.cp.lodent.Agent' " +
"       AND A.id = ogi.objectId AND A.person != null " +
"   ) as rName, " +
"   ( " +
"       SELECT A.person.passSeria FROM Agent as A " +
"       WHERE ogi.objectClass = 'org.cp.lodent.Agent' AND A.id = ogi.objectId AND A.person != null " +
"   ) as rDocSeries, " +
"   ( " +
"       SELECT A.person.passNo FROM Agent as A " +
"       WHERE ogi.objectClass = 'org.cp.lodent.Agent' AND A.id = ogi.objectId AND A.person != null " +
"   ) as rDocNumber, " +
"   ( " +
"       SELECT A.person.passDate FROM Agent as A " +
"       WHERE ogi.objectClass = 'org.cp.lodent.Agent' AND A.id = ogi.objectId AND A.person != null " +
"   ) as rDocIssueDate, " +
"   ( " +
"       SELECT A.person.passFrom FROM Agent as A " +
"       WHERE ogi.objectClass = 'org.cp.lodent.Agent' AND A.id = ogi.objectId AND A.person != null " +
"   ) as rDocIssueOrg, " +
"   ( " +
"       SELECT A.person.factAddress FROM Agent as A "+
"       WHERE ogi.objectClass = 'org.cp.lodent.Agent' AND A.id = ogi.objectId AND A.person != null " +
"   ) as rAddress, " +
"   ( " +
"       SELECT P.sex FROM Person as P " +
"       WHERE ogi.objectClass = 'org.cp.model.common.Person' " +
"       AND P.id = ogi.objectId " +
"   ) as pSex, " +
"   ( " +
"       SELECT A.person.sex FROM Agent as A "+
"       WHERE ogi.objectClass = 'org.cp.lodent.Agent' AND A.id = ogi.objectId AND A.person != null " +
"   ) as rSex, " +
"   ( " +
"       SELECT P.identityDocType FROM Person as P "+
"       WHERE ogi.objectClass='org.cp.model.common.Person' AND P.id = ogi.objectId " +
"   ) as pDocType, " +
"   ( " +
"       SELECT A.person.identityDocType FROM Agent as A " +
"       WHERE ogi.objectClass = 'org.cp.lodent.Agent' AND A.id = ogi.objectId AND A.person != null " +
"   ) as rDocType, " +
"   dp1 " +
" FROM User u1, ObjectGroupItem ogi, GossrvcDocPackage dp1 " +
" WHERE u1.login = :login " +
" AND ogi.objectGroup.id = :Zayavitel " +
" AND ogi.selected = true " +
" AND dp1.id = :Komplekt_doc "
)
.setParameter("login", params["user"])
.setParameter("Zayavitel", Zayavitel)
.setParameter("Komplekt_doc", Komplekt_doc)
.getResultList();

totalrows = result.size();
currentrow = 0;]]></method>
            <method name="fetch"><![CDATA[if (currentrow >= totalrows) {
    return(false);
}
var rowValue = result.get(currentrow);
if(rowValue[0] != null) {
    row["pName"] = rowValue[0];
    row["pAddress"] = rowValue[1];
    row["pDocSeries"] = rowValue[2];
    row["pDocNumber"] = rowValue[3];
    row["pDocIssueDate"] = rowValue[4];
    row["pDocIssueOrg"] = rowValue[5];
    row["pSex"] = rowValue[12];
    row["pDocType"] = rowValue[14];
} else if(rowValue[6] != null) {
    row["pName"] = rowValue[6];
    row["pAddress"] = rowValue[7];
    row["pDocSeries"] = rowValue[8];
    row["pDocNumber"] = rowValue[9];
    row["pDocIssueDate"] = rowValue[10];
    row["pDocIssueOrg"] = rowValue[11];
    row["pSex"] = rowValue[13];
    row["pDocType"] = rowValue[15];
}

row["ogvName"] = rowValue[16].getOgv().getName();
id = rowValue[16].getOgv().getId();
row["ogvAddress"] = clazz.getEntityManagerLod().createQuery(
" SELECT cefv.fieldStringValue " +
" FROM CardElementFieldValue cefv " +
" WHERE cefv.cardField.fieldName = 'addressOGV' " +
" AND cefv.cardElement.id = :id "
).setParameter("id", id).getSingleResult();
if(!row["ogvAddress"].equals("")) {
    row["ogv"] = row["ogvName"] + " " + "(" + row["ogvAddress"] + ")";
} else {
    row["ogv"] = row["ogvName"];
}

currentrow = currentrow + 1;
return(true);]]></method>
