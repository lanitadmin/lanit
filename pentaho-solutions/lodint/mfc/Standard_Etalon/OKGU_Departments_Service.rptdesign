            <property name="dataSource">Data Source</property>
            <method name="open"><![CDATA[clazz = new Packages.org.cp.birt.plugins.Shark();
clazzConvert = new Packages.org.cp.birt.plugins.Converter();

stDate = clazzConvert.getOutDate(params["startDate"]);
edDate = clazzConvert.getOutDate(params["endDate"]);

result = clazz.getEntityManagerLod().createQuery
( 
"select "+
"  rqs.ogvName, quest.nameQuestion, answ.nameAnswer, answ.numberValue, rqs.dateTime, rqs.serviceName  "+
"from GossrvcPackRatingQualityService rqs join rqs.listRatingQualityService quest join quest.listAnswer answ "+
"where "+ 
"     rqs.dateTime between :stDate and :edDate  "

).setParameter("stDate", stDate)
.setParameter("edDate", edDate)
.getResultList();
	

 currentrow=0;
 totalrows = result.size();
 ]]></method>
            <method name="fetch"><![CDATA[if (currentrow >= totalrows) {
	return(false);
}

var rowValue = result.get(currentrow);

row["ogv_name"] = rowValue[0];

if (rowValue[1] != null) {
	row["question_text"] = rowValue[1].replaceAll("Оцените","");

}

//row["question_text"] = rowValue[1];
answer = rowValue[2];
row["numberAn"]=rowValue[3];

row["answ_vb"]=0;
row["answ_b"]=0;
row["answ_n"]=0;
row["answ_g"]=0;
row["answ_vg"]=0;

if (rowValue[3]==1) row["answ_vb"]=1 
else 
  if (rowValue[3]==2) row["answ_b"]=1
  else 
    if (rowValue[3]==3) row["answ_n"]=1
    else 
      if (rowValue[3]==4) row["answ_g"] = 1
      else row["answ_vg"] = 1;
row["dateRating"] = rowValue[4];  
row["service_name"] = rowValue[5];
currentrow = currentrow + 1;

return(true);]]></method>
