totalrows = 1;

currentrow = 0;]]></method>
            <method name="fetch"><![CDATA[if (currentrow >= totalrows) {
	return(false);
}

if(resultDate==1){
row["startDate"] = params["startDate"];
row["endDate"] = params["endDate"];
row["valid"] = 1;
}
else{
row["startDate"] = "(Неправильно определен формат даты)";
row["endDate"] = "(Неправильно определен формат даты)";
